{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "test": {
      "cache": false,
      "outputs": [],
      "inputs": ["src/**/*.ts"]
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["test", "lint"],
      "outputs": []
    }
  }
}