{
  "name": "eslint-config-liebe",
  "private": true,
  "description": "*A config by the 'team liebe' or 'team love' :heartpulse: in english that lets you love your code by lending you a hand with powerful lints.*",
  "license": "ISC",
  "homepage": "https://github.com/demvsystems/eslint-config-liebe#readme",
  "repository": "github:demvsystems/eslint-config-liebe",
  "bugs": "https://github.com/demvsystems/eslint-config-liebe/issues",
  "author": "Tjark Kühl",
  "contributors": [
    {
      "name": "Tjark Kühl",
      "url": "https://github.com/Tjark-Kuehl"
    },
    {
      "name": "Philipp Loose",
      "url": "https://github.com/phloose"
    }
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "typescript",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "esnext"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.ts,.json --fix",
    "release": "pnpm -r --workspace-concurrency=1 exec -- npx --no-install semantic-release -e semantic-release-monorepo",
    "release:dry": "pnpm -r --workspace-concurrency=1 exec -- npx --no-install semantic-release -e semantic-release-monorepo --dry-run"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "commitlint": "17.4.4",
    "eslint": "8.36.0",
    "husky": "8.0.3",
    "semantic-release": "19.0.5",
    "semantic-release-monorepo": "7.0.5"
  },
  "packageManager": "pnpm@7.29.1"
}
