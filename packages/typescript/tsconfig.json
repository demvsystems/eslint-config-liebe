{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "skipLibCheck": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "useDefineForClassFields": true,
    "noImplicitAny": false,
    "isolatedModules": true,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules"
  ]
}
